<?xml version="1.0"?>
<yandex>
    <logger>
        <level>trace</level>
        <console>true</console>
    </logger>

    <tcp_port>9000</tcp_port>

    <path>./</path>

    <mark_cache_size>0</mark_cache_size>

    <!-- One cannot overrid it via cli options, since it is reloadable -->
    <max_server_memory_usage>1000000000</max_server_memory_usage> <!-- 1G -->
    <!-- Sync with RSS earlier -->
    <asynchronous_metrics_update_period_s>1</asynchronous_metrics_update_period_s>

    <users>
        <default>
            <password></password>

            <networks>
                <ip>::/0</ip>
            </networks>

            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>

    <profiles>
        <default/>
    </profiles>

    <quotas>
        <default />
    </quotas>
</yandex>
